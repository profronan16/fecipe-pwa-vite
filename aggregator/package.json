{
  "name": "fecipe-aggregator",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "tsx src/server.ts",
    "recompute": "tsx src/worker.ts --once",
    "recompute:debug": "cross-env DEBUG_AGG_LOG=1 tsx src/worker.ts --once"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "firebase": "^12.4.0",
    "firebase-admin": "^12.5.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.2",
    "cross-env": "^10.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
